# StringViewer CMake config file
#
# This file sets the following variables:
# StringViewer_FOUND - Always TRUE.
# StringViewer_INCLUDE_DIRS - Directories containing the StringViewer include files.
# StringViewer_IDL_DIRS - Directories containing the StringViewer IDL files.
# StringViewer_LIBRARIES - Libraries needed to use StringViewer.
# StringViewer_DEFINITIONS - Compiler flags for StringViewer.
# StringViewer_VERSION - The version of StringViewer found.
# StringViewer_VERSION_MAJOR - The major version of StringViewer found.
# StringViewer_VERSION_MINOR - The minor version of StringViewer found.
# StringViewer_VERSION_REVISION - The revision version of StringViewer found.
# StringViewer_VERSION_CANDIDATE - The candidate version of StringViewer found.

message(STATUS "Found StringViewer-@PROJECT_VERSION@")
set(StringViewer_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(StringViewer_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(StringViewer_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(StringViewer_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(StringViewer_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(StringViewer_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(StringViewer_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(StringViewer_DEFINITIONS ${<dependency>_DEFINITIONS})

set(StringViewer_VERSION @PROJECT_VERSION@)
set(StringViewer_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(StringViewer_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(StringViewer_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(StringViewer_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

